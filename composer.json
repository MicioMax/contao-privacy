{
  "name": "heimrichhannot/contao-privacy",
  "description": "This bundle contains functionality concerning privacy and the European Union's \"General Data Protection Regulation\" (GDPR, in German: \"Datenschutz-Grundverordnung\", DSGVO).",
  "keywords": [
    "contao",
    "dsgvo",
    "gdpr",
    "privacy",
    "legal",
    "protocol"
  ],
  "type": "contao-module",
  "homepage": "https://github.com/heimrichhannot/contao-privacy",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Heimrich & Hannot",
      "email": "digitales@heimrich-hannot.de",
      "homepage": "http://www.heimrich-hannot.de",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/heimrichhannot/contao-privacy/issues"
  },
  "require": {
    "php": "~5.4 || ~7.0",
    "contao/core-bundle": "^3.5.1 || ~4.1",
    "contao-community-alliance/composer-plugin": "~2.4 || ~3.0",
    "heimrichhannot/contao-be_explanation": "^1.0.7",
    "heimrichhannot/contao-formhybrid": "^3.1",
    "heimrichhannot/contao-exporter": "^4.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.6 || ~6.0",
    "friendsofphp/php-cs-fixer": "^2.2",
    "satooshi/php-coveralls": "^1.0"
  },
  "autoload": {
    "classmap": []
  },
  "extra": {
    "contao": {
      "sources": {
        "": "system/modules/privacy"
      }
    }
  }
}
